   "source": [
    "np.random.seed(0)  # Set the seed for reproducibility\n",
    "\n",
    "N = len(y)  # Determining the number of data points\n",
    "\n",
    "# Generating 1000 evenly spaced values between the minimum and maximum of x, reshaped into a column vector\n",
    "x_values = np.linspace(x.min(), x.max(), 1000).reshape(-1, 1)\n",
    "\n",
    "# Extracting the optimal polynomial degree, number of neighbors, and bandwidth from the optimal results\n",
    "opt_poly_degree = int(opt_lr[0])  # Optimal polynomial degree from linear regression\n",
    "opt_k = int(opt_knn[0])  # Optimal number of neighbors from KNN\n",
    "opt_bw = float(opt_kr[0])  # Optimal bandwidth from kernel regression\n",
    "\n",
    "# Generating polynomial features for both the training data and the x_values based on the optimal polynomial degree\n",
    "x_polynomial = polynomial_features(x, opt_poly_degree)  # Polynomial features for the training data\n",
    "x_values_polynomial = polynomial_features(x_values, opt_poly_degree)  # Polynomial features for x_values\n",
    "\n",
    "# Fitting a linear regression model using the polynomial features of the training data\n",
    "lr = LinearRegression()\n",
    "lr.fit(x_polynomial, y)\n",
    "# Predicting the target values for x_values using the linear regression model\n",
    "y_pred_lr = lr.predict(x_values_polynomial)\n",
    "\n",
    "# Fitting a Gaussian kernel regression model using the optimal bandwidth\n",
    "kr = KernelReg(y, x, var_type='c', bw=np.array([opt_bw]))\n",
    "# Predicting the target values for x_values using the kernel regression model\n",
    "y_pred_kr, _ = kr.fit(x_values)\n",
    "\n",
    "# Fitting a K-Nearest Neighbors model using the optimal number of neighbors\n",
    "neigh = KNeighborsRegressor(n_neighbors=opt_k, weights='uniform')\n",
    "neigh.fit(x, y)\n",
    "# Predictting the target values for x_values using the KNN model\n",
    "y_pred_knn = neigh.predict(x_values)\n"
