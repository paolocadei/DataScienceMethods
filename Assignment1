   "source": [
    "# Finding the optimal polynomial degree based on the One-Standard-Deviation Rule,\n",
    "# with a preference for the rightmost value (left=True) (default)\n",
    "opt_lr = optimal_par(scores_dict_lr)\n",
    "\n",
    "# Find the optimal number of neighbors for K-Nearest Neighbors using the One-Standard-Deviation Rule,\n",
    "# with a preference for the rightmost value (left=False)\n",
    "opt_knn = optimal_par(scores_dict_knn, left=False)\n",
    "\n",
    "# Finding the optimal bandwidth for Gaussian kernel regression using the One-Standard-Deviation Rule,\n",
    "# with a preference for the rightmost value (left=False)\n",
    "opt_kr = optimal_par(scores_dict_kr, left=False)\n"
   ]
  },
